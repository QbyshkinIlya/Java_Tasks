<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="546.0" prefWidth="526.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.Task8.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <AnchorPane prefHeight="570.0" prefWidth="486.0">
         <children>
            <Pane fx:id="paneLight" prefHeight="200.0" prefWidth="200.0" />
            <Pane fx:id="panePresent" prefHeight="200.0" prefWidth="200.0" />
            <Pane fx:id="paneTree" prefHeight="200.0" prefWidth="200.0" />
            <Pane fx:id="paneStar" prefHeight="200.0" prefWidth="200.0" />
         </children>
      </AnchorPane>
      <HBox prefHeight="74.0" prefWidth="489.0">
         <children>

            <Button mnemonicParsing="false" onAction="#addStar" text="Звезда">
               <opaqueInsets>
                  <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
               </opaqueInsets>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#addPresents" text="Подарки">
               <opaqueInsets>
                  <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
               </opaqueInsets>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#addLights" text="Гирлянда">
               <opaqueInsets>
                  <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
               </opaqueInsets>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#addSandG" text="Гирлянда и звезда">
               <opaqueInsets>
                  <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
               </opaqueInsets>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#addAll" text="Добавить все" textFill="#1c7c14">
               <opaqueInsets>
                  <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
               </opaqueInsets>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#del" text="Удалить" textFill="RED">
               <opaqueInsets>
                  <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
               </opaqueInsets>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin></Button>
         </children>
      </HBox>
   </children>


</VBox>
